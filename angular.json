{
  "version": 1,
  "projects": {
    "heroes": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/heroes",
      "sourceRoot": "apps/heroes/src",
      "prefix": "agg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/heroes/browser",
            "index": "apps/heroes/src/index.html",
            "main": "apps/heroes/src/main.ts",
            "polyfills": "apps/heroes/src/polyfills.ts",
            "tsConfig": "apps/heroes/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "input": "libs/shared/assets",
                "glob": "**/*",
                "output": "assets"
              },
              {
                "input": "libs/shared/assets",
                "glob": "favicon.png",
                "output": "."
              },
              {
                "input": "apps/heroes/src",
                "glob": "routes.json",
                "output": "."
              },
              "apps/heroes/src/manifest.webmanifest"
            ],
            "styles": ["libs/shared/styles/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "libs/shared/environments/src/lib/environment.ts",
                  "with": "libs/shared/environments/src/lib/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/heroes/ngsw-config.json"
            },
            "ssr": {
              "fileReplacements": [
                {
                  "replace": "libs/shared/environments/src/lib/environment.ts",
                  "with": "libs/shared/environments/src/lib/environment.ssr.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/heroes/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "heroes:build",
            "proxyConfig": "apps/heroes/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "heroes:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "heroes:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/heroes/tsconfig.app.json",
              "apps/heroes/tsconfig.spec.json",
              "apps/heroes/tsconfig.server.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/heroes/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/heroes/jest.config.js",
            "passWithNoTests": true
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/heroes/server",
            "main": "apps/heroes/server.ts",
            "tsConfig": "apps/heroes/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "libs/shared/environments/src/lib/environment.ts",
                  "with": "libs/shared/environments/src/lib/environment.ssr.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "heroes:build",
            "serverTarget": "heroes:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "heroes:build:production",
              "serverTarget": "heroes:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "heroes:build:production",
            "serverTarget": "heroes:server:production",
            "routes": ["/", "/heroes"]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },

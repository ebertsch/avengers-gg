{
  "version": 1,
  "projects": {
    "heroes": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/heroes",
      "sourceRoot": "apps/heroes/src",
      "prefix": "agg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/heroes",
            "index": "apps/heroes/src/index.html",
            "main": "apps/heroes/src/main.ts",
            "polyfills": "apps/heroes/src/polyfills.ts",
            "tsConfig": "apps/heroes/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "input": "libs/shared/assets",
                "glob": "**/*",
                "output": "assets"
              },
              {
                "input": "libs/shared/assets",
                "glob": "favicon.ico",
                "output": "."
              },
              {
                "input": "apps/heroes/src",
                "glob": "routes.json",
                "output": "."
              }
            ],
            "styles": ["libs/shared/styles/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "libs/shared/environments/src/lib/environment.ts",
                  "with": "libs/shared/environments/src/lib/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "heroes:build",
            "proxyConfig": "apps/heroes/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "heroes:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "heroes:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/heroes/tsconfig.app.json",
              "apps/heroes/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/heroes/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/heroes/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
